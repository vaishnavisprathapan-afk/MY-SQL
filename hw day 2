SELECT * FROM sstudents WHERE fees_paid > 5000;

UPDATE sstudents SET status = 'Active' WHERE course = 'Web Development';

UPDATE sstudents SET fees_paid = fees_paid + 1000 WHERE course = 'Data Science';

UPDATE sstudents SET status = 'Inactive', fees_paid = fees_paid - 500 WHERE id = 3;

DELETE FROM sstudents WHERE id = 2;

DELETE FROM sstudents WHERE status = 'Inactive';